  <!-- Action -->
  <record id="action_crm_lead_tracking_log" model="ir.actions.act_window">
    <field name="name">Lead Location Logs</field>
    <field name="res_model">crm.lead.tracking.log</field>
    <field name="view_mode">tree,form</field>
    <field name="search_view_id" ref="view_crm_lead_tracking_log_search"/>
  </record>

  <!-- Menu -->
  <menuitem id="menu_crm_lead_tracking_log_root"
            name="Lead Tracking Logs"
            parent="crm.crm_menu_root"
