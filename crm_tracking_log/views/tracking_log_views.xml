<odoo>
    <record id="view_tracking_log_tree" model="ir.ui.view">
        <field name="name">crm.tracking.log.tree</field>
        <field name="model">crm.tracking.log</field>
        <field name="arch" type="xml">
            <tree>
                <field name="lead_id"/>
                <field name="user_id"/>
                <field name="latitude"/>
                <field name="longitude"/>
                <field name="timestamp"/>
            </tree>
        </field>
    </record>

    <record id="view_tracking_log_form" model="ir.ui.view">
        <field name="name">crm.tracking.log.form</field>
        <field name="model">crm.tracking.log</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="lead_id"/>
                        <field name="user_id"/>
                        <field name="latitude"/>
                        <field name="longitude"/>
                        <field name="timestamp"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_tracking_log" model="ir.actions.act_window">
        <field name="name">Tracking Logs</field>
        <field name="res_model">crm.tracking.log</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

    <menuitem id="menu_tracking_log_root" name="Tracking Logs" parent="crm.crm_menu_root" action="action_tracking_log"/>
</odoo>
