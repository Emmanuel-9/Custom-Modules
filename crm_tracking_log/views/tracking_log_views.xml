<odoo>
  
  <record id="view_crm_lead_tracking_log_tree" model="ir.ui.view">
    <field name="name">crm.lead.tracking.log.tree</field>
    <field name="model">crm.lead.tracking.log</field>
    <field name="arch" type="xml">
      <tree>
        <field name="lead_id"/>
        <field name="user_id"/>
        <!-- <field name="city"/> 
        <field name="country"/> -->
        <field name="latitude"/>
        <field name="longitude"/>
        <!-- <field name="status"/> -->
        <field name="date"/>
      </tree>
    </field>
  </record>

  <!-- Form View -->
  <record id="view_crm_lead_tracking_log_form" model="ir.ui.view">
    <field name="name">crm.lead.tracking.log.form</field>
    <field name="model">crm.lead.tracking.log</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="lead_id"/>
          <field name="user_id"/>
          <!-- <field name="city"/>
          <field name="country"/> -->
          <field name="latitude"/>
          <field name="longitude"/>
          <!-- <field name="status"/>
          <field name="message"/> -->
          <field name="date"/>
        </group>
      </form>
    </field>
  </record>

